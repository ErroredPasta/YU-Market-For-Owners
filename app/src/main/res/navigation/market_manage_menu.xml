<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/market_manage_menu"
    app:startDestination="@id/marketManageFragment">

    <fragment
        android:id="@+id/marketManageFragment"
        android:name="com.example.yumarketforowners.marketmanage.market.presentation.screen.MarketManageFragment"
        android:label="MarketManageFragment">
        <action
            android:id="@+id/action_marketManageFragment_to_updateMarketActivity"
            app:destination="@id/updateMarketActivity" />
        <action
            android:id="@+id/action_marketManageFragment_to_noticeActivity"
            app:destination="@id/noticeActivity" />
        <action
            android:id="@+id/action_marketManageFragment_to_updateProfileActivity"
            app:destination="@id/updateProfileActivity" />
        <action
            android:id="@+id/action_marketManageFragment_to_customerSupportActivity"
            app:destination="@id/customerSupportActivity" />
    </fragment>


    <activity
        android:id="@+id/updateMarketActivity"
        android:name="com.example.yumarketforowners.marketmanage.market.presentation.screen.updatemarket.UpdateMarketActivity"
        android:label="UpdateMarketActivity">

    </activity>

    <activity
        android:id="@+id/noticeActivity"
        android:name="com.example.yumarketforowners.marketmanage.notice.presentation.screen.NoticeActivity"
        android:label="NoticeActivity" />
    <activity
        android:id="@+id/updateProfileActivity"
        android:name="com.example.yumarketforowners.marketmanage.profile.presentation.screen.UpdateProfileActivity"
        android:label="UpdateProfileActivity" />
    <activity
        android:id="@+id/customerSupportActivity"
        android:name="com.example.yumarketforowners.marketmanage.customersupport.presentation.screen.CustomerSupportActivity"
        android:label="CustomerSupportActivity" />


</navigation>