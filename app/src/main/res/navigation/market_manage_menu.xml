<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/market_manage_menu"
    app:startDestination="@id/marketManageFragment">

    <fragment
        android:id="@+id/marketManageFragment"
        android:name="com.example.yumarketforowners.presentation.screen.marketmanage.MarketManageFragment"
        android:label="MarketManageFragment">
        <action
            android:id="@+id/action_marketManageFragment_to_updateMarketActivity"
            app:destination="@id/updateMarketActivity" />
        <action
            android:id="@+id/action_marketManageFragment_to_noticeActivity"
            app:destination="@id/noticeActivity" />
    </fragment>


    <activity
        android:id="@+id/updateMarketActivity"
        android:name="com.example.yumarketforowners.presentation.screen.marketmanage.updatemarket.UpdateMarketActivity"
        android:label="UpdateMarketActivity">

    </activity>

    <activity
        android:id="@+id/noticeActivity"
        android:name="com.example.yumarketforowners.presentation.screen.marketmanage.notice.NoticeActivity"
        android:label="NoticeActivity" />


</navigation>